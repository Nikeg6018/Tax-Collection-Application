<!-- Include Header -->
<%- include('include/_header')%>
    <!-- /Include Header -->

    <!-- Include Main Navbar -->
    <%- include('include/_Main-navbar')%>
        <!-- /Include Main Navbar -->

        <div class="bg-warning">
            <div class="container">
                <div class="nav-menu-user">
                    <div class="person-intro">
                        Welcome,
                        <a class="userEdit" href="/admin/dashBoard/adminProfile">
                            <b id="person">
                                <%= User.name%>
                            </b>
                        </a>
                        <div class="space-900"></div>
                        <a href="/admin/dashBoard" class="User-details-for-admin"><i class="fa-solid fa-house"></i>
                            DashBoard</a>
                    </div>
                </div>
            </div>
        </div>

        <main>
            <div class="banner">
                <div class="hid"></div>
                <div class="container pt-5 main-container ">
                    <span>
                        <strong class="purpel-text">Add Property</strong>
                    </span>
                    <hr>

                    <div class="addpropertyList">
                        <hr>
                        <div class="Center-text"><Strong>Add Property List</Strong></div>
                        <hr>
                        <div class="addList">
                            <% if (addPropertyList.length> 0) { %>

                                <% for( let i=0; i < addPropertyList.length; i++ ) { %>
                                    <div class="container">
                                        <div class="name">
                                            <strong>Aadhar Number : </strong>
                                            <div>
                                                <%= addPropertyList[i].aadhar %>
                                            </div>
                                        </div>
                                        <div class="email">
                                            <strong>Email : </strong>
                                            <div>
                                                <%= addPropertyList[i].email %>
                                            </div>
                                        </div>
                                        <div class="number">
                                            <strong>Property Number : </strong>
                                            <div>
                                                <%= addPropertyList[i].tenment %>
                                            </div>
                                        </div>
                                        <div class="number">
                                            <strong>Photo : </strong>
                                            <div>
                                                <img src="/Docs/111-A-11-1111-1-115-photo.jpeg" alt="No img"
                                                    weight="125px" height="125px">
                                            </div>
                                        </div>
                                        <div class="number">
                                            <strong>Property Document :
                                            </strong>
                                            <div>
                                                <iframe width="100%" height="500px"
                                                    src="<%= LOCAL_URL %>/Docs/<%= addPropertyList[i].propertyDocument %>"
                                                    autoplay="0" frameborder="0" allowfullscreen></iframe>
                                                <canvas id="pdf-canvas"></canvas>
                                            </div>
                                        </div>
                                        <div class="number">
                                            <button type="button" class="btn btn-outline-success me-2">Approve</button>
                                            <button type="button" class="btn btn-outline-danger">Cancel</button>
                                        </div>
                                    </div>
                                    <% } %>
                                        <% } else {%>
                                            <div class="Center-text"><strong>No Data</strong></div>
                                            <% } %>
                        </div>

                    </div>
                    <hr>
                    <div class="sell">
                        <hr>
                        <div class="Center-text"><Strong>Sell Property List</Strong></div>
                        <hr>
                        <div class="sellList">
                            <% if (sellList.length> 0) { %>

                                <% } else { %>
                                    <div class="Center-text"><strong>No Data</strong></div>
                                    <% } %>
                        </div>
                    </div>

                </div>
            </div>

        </main>

        <script src="https://mozilla.github.io/pdf.js/build/pdf.js"></script>
        <script>
            var pdfCanvas = document.getElementById('pdf-canvas');
            var pdfContext = pdfCanvas.getContext('2d');

            PDFJS.getDocument('path/to/your/file.pdf').then(function (pdf) {
                pdf.getPage(1).then(function (page) {
                    pdfCanvas.width = page.getViewport(1).width;
                    pdfCanvas.height = page.getViewport(1).height;
                    page.render({ canvasContext: pdfContext, viewport: page.getViewport(1) });
                });
            });

        </script>

        <!-- Include Footer -->
        <%- include('include/_footer')%>
            <!-- /Include Footer -->